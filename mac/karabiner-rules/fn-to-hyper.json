{
  "title": "Change Fn+Keys to Hyper+Keys",
  "rules": [
    {
      "description": "Change Fn+h to Hyper+h",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ]
        }
      ]
    },
    {
